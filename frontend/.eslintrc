// 0 - ignore
// 1 - warn
// 2 - error
{
 "globals": {
   "angular": true,
   "_": true,
   "window": true,
   "$": true,
   "FormData":true,
   "moment":true,
   "localStorage":true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": 0,
    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "consistent-return": 0,
    "default-case": 0,
    "dot-notation": 0,
    "eqeqeq": 0,
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 0,
    "no-console": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-extra-semi": 0,
    "no-floating-decimal": 0,
    "no-implied-eval": 0,
    "no-iterator": 0,
    "no-labels": 0,
    "no-lone-blocks": 0,
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-native-reassign": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-new": 0,
    "no-octal-escape": 0,
    "no-octal": 0,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 0,
    "no-return-assign": 0,
    "no-script-url": 0,
    "no-sequences": 0,
    "no-unused-expressions": [0, {"allowShortCircuit": true }],
    "no-unused-vars": 0,
    "no-useless-call": 0,
    "no-useless-concat": 0,
    "no-void": 0,
    "no-shadow": 0,
    "no-undefined": 0,
    "no-unsafe-finally":0,
    "no-cond-assign":0,
    "no-undef":0,

      "no-use-before-define": [0, {"functions": false}],
    "brace-style": 0,
    "max-len": [0, 300],
    "max-lines": [0, 350],
    "max-nested-callbacks": [0, 5],
    "max-params": [0, 7],
    "comma-spacing": 0,
    "no-array-constructor": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-spaced-func": 0,
    "no-trailing-spaces": 0,
    "no-whitespace-before-property": 0,
    "array-callback-return": 0,
    "no-warning-comments": [ 0, {
      "terms": [
        "todo",
        "warning",
        "fixme",
        "hack",
        "xxx"
      ],
      "location": "start"
    }],
    "no-with": 0,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": [ 0, "inside" ]
  }
}
